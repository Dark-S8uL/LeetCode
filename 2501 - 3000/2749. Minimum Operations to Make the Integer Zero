class Solution {
public:
    long long cntOne(long long n) {
        long long cnt = 0 ;
        while(n) {
            cnt += (n % 2) ;
            n /= 2 ;
        }
        return cnt ;
    }
    int makeTheIntegerZero(int num1, int num2) {
        for(int i = 1; i <= 60; i++) {
            long long num = (long long)num1 - (1LL * i * num2) ;
            if(num <= 0) break ;
            if((num >= i) && (cntOne(num) <= i)) return i ;
        }
        return -1 ;
    }
};
