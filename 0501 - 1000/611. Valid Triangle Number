class Solution {
public:
    int triangleNumber(vector<int>& nums) {
        int n = nums.size() ;
        sort(nums.begin(), nums.end()) ;
        int cnt = 0 ;
        for(int i = 2; i < n; i++){
            int s = 0, e = i-1 ;
            while(s < e){
                if(nums[s] + nums[e] > nums[i]){
                    cnt += e-s ;
                    e-- ;
                } else s++ ;
            }
        }
        return cnt ;
    }
};
