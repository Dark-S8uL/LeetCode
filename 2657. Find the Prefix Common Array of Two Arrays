class Solution {
public:
    vector<int> findThePrefixCommonArray(vector<int>& a, vector<int>& b) {
        int n = a.size() ;
        vector<int> c(n) ;
        unordered_map<int, int> mp ;
        for(int i = 0; i < n; i++) {
            mp[a[i]]++ ;
            mp[b[i]]++ ;
            int cnt = 0 ;
            for(auto& pair : mp) {
                if(pair.second == 2) cnt++ ;
            }
            c[i] = cnt ;
        }
        return c ;
    }
};
