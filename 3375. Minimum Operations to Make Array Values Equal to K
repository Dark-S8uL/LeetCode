class Solution {
public:
    int minOperations(vector<int>& nums, int k) {
        int n = nums.size() ;
        sort(nums.begin(), nums.end()) ;
        if(nums[0] < k) return -1 ;
        unordered_map<int, int> mp ;
        for(int num : nums) mp[num]++ ;
        int cnt = 0 ;
        for(auto& pair : mp){
            if(pair.first > k) cnt++ ;
        }
        return cnt ;
    }
};
