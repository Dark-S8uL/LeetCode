
class Solution {
public:
    long long countSubarrays(vector<int>& nums, long long k) {
        int n = nums.size() ;
        long long cnt = 0, sc = 0 ;
        int i = 0, j = 0 ;
        for(int j = 0; j < n; j++) {
            sc += nums[j] ;
            while((sc * (j - i + 1)) >= k) {
                sc -= nums[i] ;
                i++ ;
            }
            cnt += (j - i + 1) ;
        }
        return cnt ;
    }
};
