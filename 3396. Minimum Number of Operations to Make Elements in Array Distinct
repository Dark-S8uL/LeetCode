class Solution {
public:
    int minimumOperations(vector<int>& nums) {
        int n = nums.size() ;
        unordered_map<int, int> mp ;
        int i = n-1 ;
        for(; i >= 0; i--) {
            if(mp.find(nums[i]) != mp.end()) break ;
            else mp[nums[i]]++ ;
        }
        if(i < 0) return 0 ;
        if((i+1) % 3 == 0) return (i+1) / 3 ;
        return ((i+1) / 3) + 1 ;
    }
};
